<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <!-- Note: For Android please see also Platforms\Android\Resources\values\colors.xml -->

    <Color x:Key="Primary">#512BD4</Color>

    <Color x:Key="TextPrimary">#000000</Color>
    <Color x:Key="TextSecondary">#ffffff</Color>
    <Color x:Key="TextYellow">#ffd86e</Color>

    <Color x:Key="BackgroundPrimary">#131313</Color>
    <Color x:Key="BackgroundSecondary">#232222</Color>
    <Color x:Key="BackgroundOpacityWhite">#40FFFFFF</Color>

    <Color x:Key="BorderOpacityWhite">#90FFFFFF</Color>
    <Color x:Key="BorderPink">#b14ffb</Color>

    <Color x:Key="Red">#d00000</Color>
    <Color x:Key="Yellow">#ebc500</Color>
    <Color x:Key="Green">#008c09</Color>
    <Color x:Key="White">#ffffff</Color>
</ResourceDictionary>